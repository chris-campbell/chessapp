<div class="booyah-box">

  <div class="create-game">
    <div class="row">
      <div class="col-12">
        <h1>Create Game</h1>
      </div>
      <div class="form-box row">
        <%= form_for @game, url: {action: "create"} do |f| %>
          <%= f.text_field :name %>
          <%= f.text_field :email %>
          <%= f.submit %>
        <% end %>
      </div>
    </div>
  </div>

</div>